{
  "expo": {
    "name": "BlazeMates",
    "slug": "blazemates",
    "version": "1.0.0",
    "scheme": "blazemates",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "expo-camera",
      "expo-maps",
      "expo-router",
      ["react-native-edge-to-edge", { "enabled": true }]
    ],
    "androidStatusBar": {
      "translucent": true,
      "backgroundColor": "transparent",
      "barStyle": "light-content"
    },
    "androidNavigationBar": {
      "translucent": true,
      "backgroundColor": "transparent"
    },
    "ios": {
      "bundleIdentifier": "com.hawkc13.blazemates",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We use the camera so you can take a profile photo.",
        "NSPhotoLibraryUsageDescription": "We need access to your photos to select or upload a profile picture.",
        "NSPhotoLibraryAddUsageDescription": "We save photos to your library when you choose to keep a copy."
      }
    },
    "android": {
      "package": "com.hawkc13.blazemates",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon.png",
        "backgroundColor": "#121212"
      }
    },
    "web": {
      "favicon": "./app/assets/icon.png",
      "output": "static"
    },
    "owner": "hawkc13",
    "extra": {
      "eas": {
        "projectId": "89aecb56-c6dd-4271-8f23-de709dddb2a0"
      },
      "EXPO_PUBLIC_SUPABASE_URL": "${EXPO_PUBLIC_SUPABASE_URL}",
      "EXPO_PUBLIC_SUPABASE_ANON_KEY": "${EXPO_PUBLIC_SUPABASE_ANON_KEY}"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/89aecb56-c6dd-4271-8f23-de709dddb2a0"
    }
  }
}
